{
  "template_version": "1.0.0",
  "product_name": "Cyber Learn App V1 Main",
  "repo_name": "cyber_learn_app_v1-main-platform",
  "owner_github": "anungis437",
  "primary_app_path": "src",
  "app_port": 3000,
  "tenant_key": "org_id",
  "image_repo": "cyber_learn_app_v1-main-app",
  "auth_provider": "clerk",
  "db_provider": "azure_postgresql",
  "deploy_provider": "azure_container_apps",
  "environments": {
    "staging_url": "https://cyber_learn_app_v1-main-staging.azurecontainerapps.io",
    "production_url": "https://cyber_learn_app_v1-main.azurecontainerapps.io",
    "api_base": "/api/v1"
  },
  "profile": "nodeapi-aca-azurepg-clerk",
  "modules": [
    "core-governance",
    "repo-bootstrap",
    "db-azurepg",
    "auth-clerk",
    "deploy-aca-oidc",
    "security-baseline",
    "observability-audit"
  ],
  "options": {
    "enable_ci": true,
    "enable_deploy_workflows": true,
    "enable_ai_ops": false,
    "strict_parity": true
  },
  "custom_config": {
    "key_dependencies": [],
    "feature_flags": {
      "multi_tenant": true,
      "api_gateway": false,
      "background_jobs": false,
      "real_time": false,
      "file_storage": false,
      "caching": false
    }
  },
  "migration": {
    "source_framework": null,
    "source_orm": null,
    "entity_count": 0,
    "models_count": 0,
    "migrations_count": 0,
    "has_rls": false,
    "estimated_weeks": 4,
    "complexity": "MEDIUM",
    "auth_migration": {
      "from": "custom",
      "to": "clerk",
      "complexity": "HIGH"
    },
    "priorities": [
      "Phase 1: Infrastructure Setup (Azure resources, networking)",
      "Phase 2: Database Migration (schema + data)",
      "Phase 3: Auth Migration (Clerk integration + user sync)",
      "Phase 4: Application Code (API routes, business logic)",
      "Phase 5: Frontend Migration (UI components, pages)",
      "Phase 6: Testing & Validation (E2E, integration, performance)",
      "Phase 7: Production Cutover (DNS, monitoring, rollback plan)"
    ]
  }
}